{
  "name": "",
  "version": "0.0.1",
  "private": true,
  "main": "src/electron.cjs",
  "scripts": {
    "dev": "npm run setEnvDev && npm run dev:all",
    "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \" npm run dev:svelte \" \" npm run dev:electron \"",
    "dev:svelte": "vite dev",
    "dev:electron": "electron src/electron.cjs",
    "build": "npm run setEnvProd && npm build:svelte && npm run build:electron",
    "build: svelte": "vite build",
    "build:electron": "electron-builder -mw1 --config build.config.json",
    "setEnvDev": "dotenv set NODE_ENV development",
    "setEnvProd": "dotenv set NODE_ENV production"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "2.9.0",
    "@skeletonlabs/tw-plugin": "0.3.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "@types/eslint": "^8.56.0",
    "autoprefixer": "10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^29.1.6",
    "electron-reloader": "^1.2.3",
    "electron-serve": "^1.3.0",
    "electron-window-state": "^5.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "3.4.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "0.2.1"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.6.3",
    "dotenv": "^16.4.5"
  }
}
